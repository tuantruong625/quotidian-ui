{
  "elevation": {
    "0": { "$value": "none", "$type": "boxShadow" },
    "1": { "$value": "0 1px 2px 0 rgba(0, 0, 0, 0.05)", "$type": "boxShadow" },
    "2": { "$value": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1)", "$type": "boxShadow" },
    "3": { "$value": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)", "$type": "boxShadow" },
    "4": { "$value": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1)", "$type": "boxShadow" }
  },
  "radius": {
    "none": { "$value": "0px", "$type": "borderRadius" },
    "sm":   { "$value": "4px", "$type": "borderRadius" },
    "md":   { "$value": "6px", "$type": "borderRadius" },
    "lg":   { "$value": "8px", "$type": "borderRadius" },
    "xl":   { "$value": "12px", "$type": "borderRadius" },
    "full": { "$value": "9999px", "$type": "borderRadius" }
  }
}
